# Copy this file to `.env` (gitignored) and fill values, or use scripts in `scripts/`.
#
# DB connection used by SQLAlchemy/Alembic:
#   postgresql+psycopg2://USER:PASSWORD@HOST:5432/DBNAME?sslmode=require
DATABASE_URL=postgresql+psycopg2://incidentfox:REPLACE_ME@REPLACE_ME:5432/incidentfox_config?sslmode=require

# Token hashing pepper (required for opaque team tokens auth)
# Generate with: python3 -c "import secrets; print(secrets.token_urlsafe(32))"
TOKEN_PEPPER=REPLACE_ME

# Used by scripts (optional)
AWS_PROFILE=playground
AWS_REGION=us-west-2
RDS_SECRET_ARN=arn:aws:secretsmanager:us-west-2:103002841599:secret:incidentfox-config-service/rds


